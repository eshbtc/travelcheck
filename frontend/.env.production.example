# Travel Check Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# Application URL
NEXTAUTH_URL=https://yourdomain.com
NEXTAUTH_SECRET=your-nextauth-secret-generate-a-strong-random-string

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Google Cloud Configuration (Method 1 - Recommended)
# Service Account JSON as a single environment variable
GOOGLE_APPLICATION_CREDENTIALS_JSON={"type":"service_account","project_id":"your-project","private_key_id":"...","private_key":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n","client_email":"service@your-project.iam.gserviceaccount.com","client_id":"...","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"https://www.googleapis.com/robot/v1/metadata/x509/service%40your-project.iam.gserviceaccount.com"}

# Google Cloud Configuration (Method 2 - Alternative)
# Individual service account fields (use if JSON method doesn't work)
# GOOGLE_CLOUD_PROJECT_ID=your-google-cloud-project
# GOOGLE_CLOUD_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\nYour-Private-Key-Here\n-----END PRIVATE KEY-----
# GOOGLE_CLOUD_CLIENT_EMAIL=service@your-project.iam.gserviceaccount.com

# Google AI Configuration
# Get this from https://aistudio.google.com/app/apikey
GOOGLE_AI_API_KEY=AIzaSyA...

# Document AI Configuration
DOCUMENT_AI_PROJECT_ID=your-google-cloud-project
DOCUMENT_AI_LOCATION=us
DOCUMENT_AI_PROCESSOR_ID=your-processor-id-from-document-ai

# Optional: Application Configuration
NODE_ENV=production
NEXT_PUBLIC_APP_ENV=production

# Optional: Monitoring and Analytics
# Add your monitoring service keys here
# SENTRY_DSN=https://...
# GOOGLE_ANALYTICS_ID=GA-...
# VERCEL_ANALYTICS_ID=...